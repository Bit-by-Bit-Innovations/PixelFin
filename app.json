{
  "expo": {
    "name": "PixelFin",
    "slug": "pixelfin",
    "scheme": "pixelfin",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0b0d0f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pixelfin.app",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": []
  }
}
